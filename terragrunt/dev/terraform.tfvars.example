# terraform.tfvars.example
#
# Copy this file to 'terraform.tfvars' and fill in the values.
# These variables override the defaults in terragrunt.hcl.

# Naming and location
# project_name = "sampleapp"
# environment  = "dev"
# location     = "East US"

# Docker Image Details
# The docker_image will be dynamically set based on the ACR created.
# You will need to update the terragrunt.hcl file with the correct ACR login server.
# docker_image_tag = "latest"

# App Service Environment Variables
app_settings = {
  "APP_NAME"      = "Laravel"
  "APP_ENV"       = "production"
  "APP_KEY"       = "base64:PLEASE_GENERATE_YOUR_OWN_KEY" # Run `php artisan key:generate --show` and paste the result here
  "APP_URL"       = "https://<your-app-name>.azurewebsites.net" # Update with your app's future URL
  "LOG_CHANNEL"   = "stack"
  "DB_CONNECTION" = "mysql"
  "DB_HOST"       = "127.0.0.1" # Consider using Azure Database for MySQL
  "DB_PORT"       = "3306"
  "DB_DATABASE"   = "laravel"
  "DB_USERNAME"   = "root"
  "DB_PASSWORD"   = ""
}
